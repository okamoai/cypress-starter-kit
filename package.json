{
  "name": "cypress-starter-kit",
  "version": "0.1.0",
  "description": "Cypress x TypeScript Starter Kit",
  "scripts": {
    "clean:result": "node -e \"fs.rmSync('cypress/result', { recursive: true, force: true })\"",
    "clean:report": "node -e \"fs.rmSync('cypress/report', { recursive: true, force: true })\"",
    "open:production": "cypress open -C cypress/config/production.json",
    "test:production": "npm run clean:result && cypress run -C cypress/config/production.json",
    "open:staging": "cypress open -C cypress/config/staging.json",
    "test:staging": "npm run clean:result && cypress run -C cypress/config/staging.json",
    "open:development": "cypress open -C cypress/config/development.json",
    "test:development": "npm run clean:result && cypress run -C cypress/config/development.json",
    "report:merge": "mochawesome-merge cypress/result/json/*.json > cypress/result/merged.json",
    "report:generate": "marge cypress/result/merged.json --reportDir cypress/report --assetsDir cypress/report/assets --reportFilename index --charts",
    "report:copyMedia": "ts-node ./libs/copyMedia.ts",
    "report": "npm run clean:report && npm run report:merge && npm run report:generate && npm run report:copyMedia",
    "eslint": "eslint --ext js,ts .",
    "eslint-fix": "eslint --fix --ext js,ts .",
    "type-check": "tsc --noEmit",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/okamoai/cypress-starter-kit.git"
  },
  "author": "okamoai <okamoai@rano-raraku.net>",
  "license": "MIT",
  "lint-staged": {
    "*.ts": "bash -c tsc",
    "*.{js,ts}": "eslint --fix"
  },
  "engines": {
    "node": "16.13.0",
    "npm": "8.1.0"
  },
  "volta": {
    "node": "16.13.0",
    "npm": "8.1.0"
  },
  "bugs": {
    "url": "https://github.com/okamoai/cypress-starter-kit/issues"
  },
  "homepage": "https://github.com/okamoai/cypress-starter-kit#readme",
  "dependencies": {
    "@cypress/skip-test": "^2.6.1",
    "@testing-library/cypress": "^8.0.2",
    "cypress": "^9.5.1",
    "cypress-multi-reporters": "^1.5.0",
    "mochawesome": "^7.1.1",
    "mochawesome-merge": "^4.2.1",
    "mochawesome-report-generator": "^6.1.0",
    "ts-node": "^10.6.0",
    "typescript": "^4.6.2"
  },
  "devDependencies": {
    "@types/mochawesome": "^6.2.1",
    "@types/node": "^17.0.21",
    "@types/prettier": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "eslint": "^8.10.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.4",
    "prettier": "^2.5.1"
  }
}
